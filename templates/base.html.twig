<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>"> {% block stylesheets %}{% endblock %}

		{% block javascripts %}
			<script type="module" src="{{asset('./assets/js/script.js')}}"></script>
		{% endblock %}
	</head>

	<body>

		<header>


			<nav>
				<div>Logo</div>

				<ul id="navMenu"></ul>

			</nav>

			{% if is_granted('IS_AUTHENTICATED_FULLY') %}

				<div>
					<span>{{user.nom}}</span>
					<a href="{{path('app_logout')}}">Log Out</a>
				</div>

			{% else %}

				<div>
					<a href="{{path('app_register')}}">Register</a>
					<a href="{{path('app_login')}}">Log In</a>
				</div>

			{% endif %}

		</header>

		{% block body %}{% endblock %}

		<footer></footer>

	</body>
</html>
